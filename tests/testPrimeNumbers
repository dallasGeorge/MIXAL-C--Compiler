{
    var n : int;
    var i : int;
    var j : int;
    var isPrime : int;
    n = 100;
    i = 2;
    while (i <= n) {
        isPrime = 1;

        j = 2;
        while (j <= i / 2) {
            if (i % j == 0) {
                isPrime = 0;
                break;
            }
            j = j + 1;
        }
        if (isPrime == 1) {
            print i;
        }
        i = i + 1;
    }
}